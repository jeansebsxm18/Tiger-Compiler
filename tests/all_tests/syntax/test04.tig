/* error : syntax error, unexpected ) */

type int_array = array of int

function max_search(int_array : int_array, size : int) : int =
let
    var len := size - 1
    var max := 0
in
    if size <= 0 then
        -1
    else
        (for i := 0 to len do
            if int_array[i] > int_array[max] then
                max := i; max;)
end
